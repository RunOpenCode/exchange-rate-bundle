{
    "name": "runopencode/exchange-rate-bundle",
    "description": "Symfony integration for 'runopencode/exchange-rate' library.",
    "keywords": [
        "exchange rate", "symfony"
    ],
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Nikola Svitlica a.k.a TheCelavi",
            "email": "thecelavi@gmail.com",
            "homepage": "http://www.runopencode.com",
            "role": "Project lead"
        },
        {
            "name": "RunOpenCode members",
            "email": "support@runopencode.com",
            "homepage": "http://www.runopencode.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "RunOpenCode\\Bundle\\ExchangeRate\\": "src/RunOpenCode/Bundle/ExchangeRate/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RunOpenCode\\Bundle\\ExchangeRate\\Tests\\": "test/"
        }
    },
    "require": {
        "php": ">=5.5",
        "symfony/framework-bundle": "~2.3",
        "runopencode/exchange-rate": "dev-master",
        "symfony/dom-crawler": "~3.0",
        "symfony/css-selector": "~3.0"
    },
    "suggest": {
        "guzzlehttp/guzzle": "~6.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.8",
        "symfony/phpunit-bridge": "~2.7",
        "symfony/debug": "~2.7",
        "matthiasnoback/symfony-dependency-injection-test": "^0.7.6"
    }
}
