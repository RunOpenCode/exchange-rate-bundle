{
    "name": "runopencode/exchange-rate-bundle",
    "description": "Symfony integration for 'runopencode/exchange-rate' library.",
    "keywords": [
        "exchange rate", "symfony"
    ],
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Nikola Svitlica a.k.a TheCelavi",
            "email": "thecelavi@gmail.com",
            "homepage": "http://www.runopencode.com",
            "role": "Project lead"
        },
        {
            "name": "RunOpenCode members",
            "email": "support@runopencode.com",
            "homepage": "http://www.runopencode.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "RunOpenCode\\Bundle\\ExchangeRate\\": "src/RunOpenCode/Bundle/ExchangeRate/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RunOpenCode\\Bundle\\ExchangeRate\\Tests\\": "test/"
        },
        "classmap": [ "test/Fixtures/project/app" ]
    },
    "require": {
        "php": ">=7.0",
        "symfony/symfony": "^3.0|^4.0",
        "runopencode/exchange-rate": "^3.0",
        "symfony/validator": "^3.0|^4.0",
        "symfony/form": "3.0|^4.0",
        "roave/security-advisories": "dev-master"
    },
    "suggest": {
        "runopencode/exchange-rate-nbs": "Provides you with exchange rates from National Bank of Serbia.",
        "runopencode/exchange-rate-intesa-rs": "Provides you with exchange rates from Banca Intesa Serbia.",
        "symfony/swiftmailer-bundle": "Send e-mail about rates fetch success."
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0",
        "symfony/phpunit-bridge": "^3.0|^4.0",
        "squizlabs/php_codesniffer": "^2.0|^3.0",
        "phploc/phploc": "^3.0|^4.0",
        "pdepend/pdepend" : "^2.5",
        "phpmd/phpmd" : "@stable",
        "sebastian/phpcpd": "^3.0|^4.0",
        "theseer/phpdox": "^0.10",
        "escapestudios/symfony2-coding-standard": "^2.0|^3.0",
        "symfony/debug": "^3.0|^4.0",
        "matthiasnoback/symfony-dependency-injection-test": "^2.0|^3.0",
        "symfony/swiftmailer-bundle": "^3.0"
    }
}
